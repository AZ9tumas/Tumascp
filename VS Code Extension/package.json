{
    "name": "tmcp",
    "displayName": "Tumascript Syntax Highlighting",
    "description": "This extention will highlight the tumascp programming language syntax.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tmcp",
            "aliases": ["Tumascp", "tmcp"],
            "extensions": [".tmcp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tmcp",
            "scopeName": "source.tmcp",
            "path": "./syntaxes/tmcp.tmLanguage.json"
        }]
    }
}